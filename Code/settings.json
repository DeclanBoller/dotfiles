  "files.trimTrailingWhitespace": true,
  "peacock.affectStatusBar": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.fontLigatures": true,
  "cSpell.userWords": ["Declan", "arity", "drogon", "entrypoint", "rollbar"],
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.enabledFileTypes": {
    "HTML (EEx)": true,
    "HTML (Eex)": true,
    "asciidoc": true,
    "c": true,
    "cpp": true,
    "csharp": true,
    "css": true,
    "git-commit": true,
    "go": true,
    "handlebars": true,
    "haskell": true,
    "html": true,
    "jade": true,
    "java": true,
    "javascript": true,
    "javascriptreact": true,
    "json": true,
    "jsonc": true,
    "latex": true,
    "less": true,
    "markdown": true,
    "php": true,
    "plaintext": true,
    "pug": true,
    "python": true,
    "restructuredtext": true,
    "rust": true,
    "scala": true,
    "scss": true,
    "text": true,
    "typescript": true,
    "typescriptreact": true,
    "yaml": true,
    "yml": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "glimmer-js",
    "glimmer-ts"
  ],
  "eslint.validate": ["glimmer-ts", "glimmer-js"],
  "peacock.affectTabActiveBorder": true,
  "eslint.codeAction.disableRuleComment": {},
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "turboConsoleLog.logMessagePrefix": "DB",
  "workbench.sideBar.location": "left",
  "materialTheme.accent": "Teal",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "redhat.telemetry.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.useEditorConfig": false,
  "git.enabled": true,
  "git.path": "/usr/local/bin/hub",
  "git.confirmSync": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "peacock.affectEditorGroupBorder": true,
  "peacock.affectPanelBorder": true,
  "peacock.affectSideBarBorder": true,
  "peacock.affectStatusAndTitleBorders": true,
  "workbench.startupEditor": "none",
  "editor.inlineSuggest.enabled": true,
  "editor.guides.bracketPairs": true,
  "gitlens.defaultDateFormat": null,
  "editor.inlineSuggest.showToolbar": "always",
  "window.zoomLevel": -1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[glimmer-js]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[glimmer-ts]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "prettier.documentSelectors": ["**/*.*", "**/*.gts", "**/*.gjs"],
  "github.copilot.editor.enableAutoCompletions": true,
  "terminal.integrated.fontLigatures": true,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    },
    "zsh (2)": {
      "path": "/opt/homebrew/bin/zsh"
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh (2)",
  "terminal.external.linuxExec": "iterm",
  "terminal.external.osxExec": "iTerm.app"
}
