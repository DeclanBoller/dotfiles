  "files.trimTrailingWhitespace": true,
  "peacock.affectStatusBar": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.fontLigatures": true,
  "cSpell.userWords": ["Declan", "arity", "drogon", "entrypoint", "rollbar"],
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.enabledLanguageIds": [
    "HTML (EEx)",
    "HTML (Eex)",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "glimmer-js",
    "glimmer-ts"
  ],
  "eslint.validate": ["glimmer-ts", "glimmer-js"],
  "peacock.affectTabActiveBorder": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.disableRuleComment": {},
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "turboConsoleLog.logMessagePrefix": "DB",
  "workbench.sideBar.location": "left",
  "materialTheme.accent": "Teal",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "redhat.telemetry.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.useEditorConfig": false,
  "git.enabled": true,
  "git.path": "/usr/local/bin/hub",
  "git.confirmSync": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "kite.showWelcomeNotificationOnStartup": false,
  "bracketPairColorizer.depreciation-notice": false,
  "peacock.affectEditorGroupBorder": true,
  "peacock.affectPanelBorder": true,
  "peacock.affectSideBarBorder": true,
  "peacock.affectStatusAndTitleBorders": true,
  "workbench.startupEditor": "none",
  "tabnine.experimentalAutoImports": true,
  "editor.inlineSuggest.enabled": true,
  "editor.guides.bracketPairs": true,
  "gitlens.defaultDateFormat": null,
  "nxConsole.showNodeVersionOnStartup": false,
  "editor.inlineSuggest.showToolbar": "always",
  "window.zoomLevel": -1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[glimmer-js]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[glimmer-ts]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "prettier.documentSelectors": ["**/*.*", "**/*.gts", "**/*.gjs"],
  "github.copilot.editor.enableAutoCompletions": true
}
