      "value": "#17A398"
    },
    {
      "name": "mathseeds-prime-client",
      "value": "#3DDC97"
    },
    {
      "name": "parent-ui-client",
      "value": "#32021F"
    },
    {
      "name": "re-client",
      "value": "#372549"
    },
    {
      "name": "re-tablet",
      "value": "#074F57"
    },
    {
      "name": "readingeggs-junior-client",
      "value": "#FFFFC7"
    },
    {
      "name": "rex-client",
      "value": "#1B263B"
    },
    {
      "name": "shop-client",
      "value": "#F0803C"
    },
    {
      "name": "teacher-ui-client",
      "value": "#80A1D4"
    },
    {
      "name": "wf-student-client",
      "value": "#8B2635"
    },
    {
      "name": "wf-teacher-client",
      "value": "#D95D39"
    }
